{
  "name": "docux",
  "type": "module",
  "version": "1.0.5",
  "packageManager": "pnpm@8.5.1",
  "license": "MIT",
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "app.config.ts",
    "app.vue",
    "assets",
    "components",
    "composables",
    "layouts",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "plugins",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .starters/default",
    "dev": "nuxi dev .starters/default --port 3030",
    "build": "nuxi build .starters/default",
    "generate": "nuxi generate .starters/default",
    "preview": "nuxi preview .starters/default",
    "lint": "eslint .",
    "release": "bumping --commit --push --tag && pnpm publish"
  },
  "dependencies": {
    "@nuxt-themes/tokens": "^2.0.0",
    "@nuxt-themes/typography": "^1.0.0",
    "@nuxt/content": "^2.7.0",
    "@nuxthq/studio": "^0.13.2",
    "@nyxb/elements": "^0.0.8",
    "@vueuse/nuxt": "^10.2.0"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.17.2",
    "@docsearch/css": "^3.5.1",
    "@docsearch/js": "^3.5.1",
    "@nuxt/devtools": "^0.6.2",
    "@nuxt/eslint-config": "0.1.1",
    "@nuxtjs/algolia": "^1.8.0",
    "@types/node": "^20.3.1",
    "eslint": "^8.52.0",
    "nuxt": "^3.5.3",
    "release-it": "^15.11.0",
    "typescript": "^5.1.3",
    "vite-plugin-inspect": "^0.7.28",
    "vue": "^3.3.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0",
        "vite": "*"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios",
        "vite"
      ]
    }
  },
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
